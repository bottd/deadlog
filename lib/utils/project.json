{
	"name": "utils",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "lib/utils/src",
	"projectType": "library",
	"targets": {
		"check": {
			"executor": "nx:run-commands",
			"options": {
				"command": "tsc --noEmit",
				"cwd": "lib/utils"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["lib/utils/**/*.{ts,js}"]
			}
		}
	},
	"tags": []
}
