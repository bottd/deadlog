{
	"name": "deadlog.io",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"postinstall": "cd node_modules/deadlock-api-client && npm run build",
		"dev": "pnpm --filter 'deadlog-web' dev",
		"build": "pnpm --filter 'deadlog-web' build",
		"preview": "pnpm --filter 'deadlog-web' preview",
		"test": "pnpm --filter 'deadlog-web' test",
		"test:all": "pnpm -r --filter './app' --filter './lib/**' run test",
		"check": "pnpm --filter 'deadlog-web' check",
		"lint": "pnpm --filter 'deadlog-web' lint",
		"format": "prettier --write .",
		"fmt:check": "prettier --check .",
		"build:scraper": "OUTPUT_DIR=app/static tsx --tsconfig=tsconfig.base.json lib/scraper/src/compile.ts",
		"build:meta": "DATABASE_URL=file:app/static/deadlog.db tsx --tsconfig=tsconfig.base.json lib/meta/src/generate-previews.ts"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "4.20260228.0",
		"@eslint/js": "9.39.3",
		"@fontsource/inter": "5.2.8",
		"@libsql/client": "0.17.0",
		"@lucide/svelte": "0.575.0",
		"@internationalized/date": "3.11.0",
		"@sveltejs/adapter-cloudflare": "7.2.8",
		"@sveltejs/adapter-node": "5.5.3",
		"@sveltejs/kit": "2.53.0",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@tailwindcss/typography": "0.5.19",
		"@tailwindcss/vite": "4.2.0",
		"@takumi-rs/core": "0.68.16",
		"@takumi-rs/helpers": "0.68.16",
		"@tanstack/svelte-query": "6.0.18",
		"@testing-library/jest-dom": "6.9.1",
		"@types/node": "24.10.13",
		"@types/react": "19.2.14",
		"bits-ui": "2.16.1",
		"clsx": "2.1.1",
		"deadlock-api-client": "github:deadlock-api/openapi-clients#path:typescript/api",
		"drizzle-kit": "0.31.9",
		"drizzle-orm": "0.45.1",
		"drizzle-zod": "0.8.3",
		"eslint": "9.39.3",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.15.0",
		"globals": "17.3.0",
		"happy-dom": "20.7.0",
		"mode-watcher": "1.1.0",
		"msw": "2.12.10",
		"playwright": "1.58.2",
		"prettier": "3.8.1",
		"prettier-plugin-sql": "0.19.2",
		"prettier-plugin-svelte": "3.5.0",
		"prettier-plugin-tailwindcss": "0.7.2",
		"react": "19.2.4",
		"runed": "0.37.1",
		"svelte": "5.53.0",
		"svelte-check": "4.4.3",
		"svelte-meta-tags": "4.5.0",
		"svelte-sonner": "1.0.7",
		"tailwind-merge": "3.5.0",
		"tailwind-variants": "3.2.2",
		"tailwindcss": "4.2.0",
		"tsx": "4.21.0",
		"tw-animate-css": "1.4.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.56.0",
		"vite": "7.3.1",
		"vite-tsconfig-paths": "6.1.1",
		"@vitest/browser": "4.0.18",
		"@vitest/browser-playwright": "4.0.18",
